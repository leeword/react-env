{
  "name": "react-env",
  "version": "1.0.0",
  "description": "A template for initial react project with webpack",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "HOT_RELOAD=1 webpack-dev-server --config build/webpack.dev.js --color",
    "build": "webpack --config build/webpack.prod.js --progress",
    "build:dll": "webpack --config build/webpack.dll.js",
    "build:report": "SHOW_REPORT=1 npm run build"
  },
  "keywords": [
    "webpack4",
    "react",
    "redux",
    "axios"
  ],
  "author": "Frank Fu",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
